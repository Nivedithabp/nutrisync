{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
  "expo-router",
  [
    "expo-camera",
    {
      "cameraPermission": "Allow access to the camera",
      "microphonePermission": "Allow access to the microphone",
      "recordAudioAndroid": true
    }
  ]
]
,
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b461e4a-1d8e-40f7-ae63-c9dcfe80bc22"
      }
    },
    "android": {
      "package": "com.bpniveditha.boltexponativewind"
    }
  }
}
